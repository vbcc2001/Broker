<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<widget id="com.ytzq.broker" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <feature name="Whitelist">
    <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin"/>
    <param name="onload" value="true"/>
  </feature>
  <feature name="HotCodePush">
    <param name="android-package" value="com.nordnetab.chcp.main.HotCodePushPlugin"/>
    <param name="onload" value="true"/>
  </feature>
  <feature name="HotCodePushLocalDevMode">
    <param name="android-package" value="com.nordnetab.chcp.localdev.HotCodePushLocalDevPlugin"/>
    <param name="onload" value="true"/>
  </feature>
  <name>Broker</name>
  <description>&#xD;
        A sample Apache Cordova application that responds to the deviceready event.&#xD;
    </description>
  <author email="dev@cordova.apache.org" href="http://cordova.io">&#xD;
        Apache Cordova Team&#xD;
    </author>
  <content src="m/index.html"/>
  <access origin="*"/>
  <allow-intent href="http://*/*"/>
  <allow-intent href="https://*/*"/>
  <allow-intent href="tel:*"/>
  <allow-intent href="sms:*"/>
  <allow-intent href="mailto:*"/>
  <allow-intent href="geo:*"/>
  <allow-intent href="market:*"/>
  <chcp>
    <config-file url="http://218.18.105.125:10004/chcp.json"/>
    <local-development enabled="true"/>
  </chcp>
  <preference name="loglevel" value="DEBUG"/>
  <preference name="loadUrlTimeoutValue" value="60000"/>
</widget>